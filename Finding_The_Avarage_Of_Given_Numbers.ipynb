   "source": [
    "total_numbers = input(\"How many numbers will you enter: \")\n",
    "count = 0\n",
    "sum = 0\n",
    "while count < int(total_numbers):\n",
    "    number = int(input(\"Enter a number: \"))\n",
    "    sum += number\n",
    "    count += 1\n",
    "print(f\"Girdiğiniz {total_numbers} sayının ortalaması: {sum/int(total_numbers)}\")"
   ]
  }
